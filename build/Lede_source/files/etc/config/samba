
config samba
	option name 'OpenWrt'
	option workgroup 'WORKGROUP'
	option description 'OpenWrt'
	option homes '1'
	option autoshare '0'

config sambashare
	option auto '1'
	option path '/mnt/sdb1'
	option read_only 'no'
	option guest_ok 'yes'
	option dir_mask '0777'
	option device 'sdb1'
	option browseable 'yes'
	option name 'sdb'
	option create_mask '0777'

config sambashare
	option browseable 'yes'
	option name 'sdc'
	option path '/mnt/sdc1'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '0777'
	option dir_mask '0777'
